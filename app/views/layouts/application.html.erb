		<script>
		$(function() {
		$( "#datepicker" ).datepicker({
		changeMonth: true,
		changeYear: true,
		maxdate: "0",
		yearRange: '-100:+0'
		});
		});
		</script> -->

		<script>
			$('.fdatepicker').DatePicker();
			$("#multiselect").zmultiselect({filter:false});
		</script>

	</head>
	<body>
		<header>
			<%= render 'layouts/navigation' %>
		</header>
		<main role="main">
			<%= render 'layouts/messages' %>
			<%= yield %>
		</main>
	</body>
</html>
